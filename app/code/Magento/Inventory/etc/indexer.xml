<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Indexer/etc/indexer.xsd">
    <indexer id="inventory_stock" view_id="inventory_stock" class="Magento\Inventory\Indexer\Stock\StockIndexer">
        <title translate="true">Stocks</title>
        <description translate="true">Rebuild Stock index (MSI)</description>
    </indexer>
    <indexer id="inventory_source" view_id="inventory_source" class="Magento\Inventory\Indexer\Source\SourceIndexer">
        <title translate="true">Sources</title>
        <description translate="true">Rebuild Source index (MSI)</description>
    </indexer>
    <indexer id="inventory_source_item" view_id="inventory_source_item" class="Magento\Inventory\Indexer\SourceItem\SourceItemIndexer">
        <title translate="true">Source Items</title>
        <description translate="true">Rebuild Source Item index (MSI)</description>
    </indexer>
</config>
